# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# AI Provider API Keys
# At least GEMINI_API_KEY is required
GEMINI_API_KEY=your-gemini-api-key-here
OPENAI_API_KEY=your-openai-api-key-here  # Optional
GROK_API_KEY=your-grok-api-key-here      # Optional

# AI Model Identifiers (optional, defaults provided)
GEMINI_AI=gemini-2.0-flash-exp
OPENAI_AI=gpt-4o-mini
GROK_AI=grok-beta

# Server Configuration
PORT=3001
NODE_ENV=development

# Database (optional, for direct connection)
DATABASE_URL=postgresql://user:password@localhost:5432/dbname
